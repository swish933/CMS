<h2>Upload Logo</h2>

<div class="container w-50 mt-4 border p-3 border-primary">
    <form action="/logo" method="POST" enctype="multipart/form-data">
        <div class="form-group">
            <label for="file">Logo input</label>
            <input name="uploadedFile" type="file" class="form-control-file" id="file">
        </div>
        <div class="form-group">
            <label for="fileName">Name</label>
            <input name='name' type="text" class="form-control" id="fileName" required />
        </div>
        <div class="form-group form-check">
            <input name="active" type="checkbox" class="form-check-input" id="logoActive" />
            <label class="form-check-label" for="logoActive">Active</label>
        </div>
        <button type="submit" class="btn btn-outline-primary">Upload Logo</button>
    </form>
</div>